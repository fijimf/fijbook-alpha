GET        /deepfij/admin/schema                       controllers.SchemaHelper.dumpSchema
GET        /deepfij/admin/schema-user                  controllers.SchemaHelper.dumpUserSchema
GET        /deepfij/admin/dropCreateSchema             controllers.SchemaHelper.dropCreateSchema
GET        /deepfij/admin/dropCreateSchema-user        controllers.SchemaHelper.dropCreateUserSchema
GET        /deepfij/admin/scrapeTeams                  controllers.ScraperController.scrapeTeams
GET        /deepfij/admin/browseTeams                  controllers.EditController.browseTeams
GET        /deepfij/admin/editTeam/:id                 controllers.EditController.editTeam(id:Long)
POST       /deepfij/admin/saveTeam                     controllers.EditController.saveTeam

GET        /assets/*file                               controllers.Assets.at(path="/public", file)

GET         /deepfij/signOut                         controllers.SignOutController.signOut
GET         /deepfij/authenticate/:provider          controllers.SocialAuthController.authenticate(provider)

